:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x5608e9b61450 .scope module, "test" "test" 2 3;
 .timescale 0 0;
v0x5608e9b73260_0 .var "clk", 0 0;
v0x5608e9b73330_0 .var "reset", 0 0;
v0x5608e9b73400_0 .net "value", 7 0, v0x5608e9b73030_0;  1 drivers
S_0x5608e9b615e0 .scope module, "c1" "counter" 2 17, 3 1 0, S_0x5608e9b61450;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x5608e9b617c0 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
v0x5608e9b4ed10_0 .net "clk", 0 0, v0x5608e9b73260_0;  1 drivers
v0x5608e9b73030_0 .var "out", 7 0;
v0x5608e9b73110_0 .net "reset", 0 0, v0x5608e9b73330_0;  1 drivers
E_0x5608e9b62180 .event posedge, v0x5608e9b73110_0, v0x5608e9b4ed10_0;
    .scope S_0x5608e9b615e0;
T_0 ;
    %wait E_0x5608e9b62180;
    %load/vec4 v0x5608e9b73110_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5608e9b73030_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5608e9b73030_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5608e9b73030_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5608e9b61450;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5608e9b73330_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5608e9b73260_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5608e9b61450;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5608e9b73330_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5608e9b73330_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5608e9b73330_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5608e9b73330_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5608e9b61450;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x5608e9b73260_0;
    %nor/r;
    %store/vec4 v0x5608e9b73260_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5608e9b61450;
T_4 ;
    %vpi_call 2 20 "$monitor", "At time %t, value = %h (%0d)", $time, v0x5608e9b73400_0, v0x5608e9b73400_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
